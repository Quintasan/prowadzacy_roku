%h1 Listing votes

%table
  %thead
    %tr
      %th
      %th
      %th

  %tbody
    - votes.each do |vote|
      %tr
        %td= link_to 'Show', admin_vote_path(vote)
        %td= link_to 'Edit', edit_admin_vote_path(vote)
        %td= link_to 'Destroy', admin_vote_path(vote), :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Vote', new_admin_vote_path
